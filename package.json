{
    "name": "@untitled-llc/unstyled",
    "description": "Internal React component library",
    "files": [
        "dist"
    ],
    "main": "./dist/unstyled.umd.js",
    "module": "./dist/unstyled.es.js",
    "exports": {
        ".": {
            "import": "./dist/unstyled.es.js",
            "require": "./dist/unstyled.umd.js"
        }
    },
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "jest",
        "eslint": "eslint \"**/*.+(js|jsx|ts|tsx)\"",
        "eslint:fix": "eslint --fix \"**/*.+(js|jsx|ts|tsx)\"",
        "prettier": "prettier --check \"**/*.+(js|jsx|ts|tsx)\"",
        "prettier:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx)\"",
        "prepare": "husky install",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "dependencies": {
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@storybook/addon-actions": "^6.5.15",
        "@storybook/addon-essentials": "^6.5.15",
        "@storybook/addon-interactions": "^6.5.15",
        "@storybook/addon-links": "^6.5.15",
        "@storybook/builder-vite": "^0.2.7",
        "@storybook/react": "^6.5.15",
        "@storybook/testing-library": "^0.0.13",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@typescript-eslint/parser": "^5.48.1",
        "@vitejs/plugin-react-swc": "^3.0.0",
        "autoprefixer": "^10.4.13",
        "babel-loader": "^8.3.0",
        "eslint": "^8.31.0",
        "eslint-config-airbnb": "^19.0.0",
        "eslint-config-next": "^13.1.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-storybook": "^0.6.10",
        "husky": "^7.0.0",
        "jest": "^27.4.7",
        "postcss": "^8.4.21",
        "prettier": "^2.7.1",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.9.4",
        "vite": "^4.0.0"
    }
}
